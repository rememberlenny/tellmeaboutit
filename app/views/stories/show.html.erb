<h1>Show Story</h1>


<h2><%= @story.name %> - <%= @story.id %></h2>

<% if !@recordings.nil? %>
  <ul>
    <% @recordings.each do |recording| %>
      <li>
        <%= recording.id %> -
        <%= link_to recording.source, story_recording_path(@story, recording) %>
      </li>
    <% end %>
  </ul>
<% end %>

<%= link_to 'Add Recording', new_story_recording_path(@story) %>
