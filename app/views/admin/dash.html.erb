<div class="row">

<h1>Admin</h1>
<div class="panel">
  <h2>Accounts</h2>
  <% @accounts.each do |account| %>
    <div class="row">
      <%= account.to_json %>
    </div>
  <% end %>
</div>

<div class="panel">
  <h2>Stories</h2>
  <% @stories.each do |story| %>
    <div class="row">
      <%= story.to_json %>
    </div>
  <% end %>
</div>

<div class="row">

  <h3>Options</h3>
  <%= link_to "View all recordings", recordings_path, :class=>"button secondary"%>
  <%= link_to "View all stories", stories_path, :class=>"button secondary"%>
</div>
